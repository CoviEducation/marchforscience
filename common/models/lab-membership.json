{
  "name": "LabMembership",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "type": {
      "type": "string",
      "required": true,
      "enum": [ "admin", "member" ]
    }
  },
  "validations": [],
  "relations": {
    "lab": {
      "type": "belongsTo",
      "model": "lab",
      "foreignKey": "labId"
    },
    "admin": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "property": "*"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "LabAdmin",
      "permission": "ALLOW",
      "property": "*"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "SuperAdmin",
      "permission": "ALLOW",
      "property": "*"
    }
  ],
  "methods": {}
}
